<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.baduncle.thediary">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MEDIA_CONTENT_CONTROL" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_right"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_right"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Listenansicht"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Kalenderansicht" />
        <activity
            android:name=".NeuerEintrag"
            android:screenOrientation="portrait" />
        <activity android:name=".Test" />
        <activity android:name=".Detailansicht" />
        <activity android:name=".EintragEdit" />
        <activity android:name=".SearchActivity"></activity>
    </application>

</manifest>